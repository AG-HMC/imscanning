<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<SelectDialog
		title="{i18n>StorageTypes}"
		items="{
			path: 'valueHelpModel>/StorageTypeVHSet'
		}"
		search=".onValueHelpSearch($event,'ST')"
		confirm=".onValueHelpConfirm($event,'ST')"
		cancel=".onValueHelpClose">
		<StandardListItem
			title="{valueHelpModel>StorageType}"
			description="{valueHelpModel>StorageType_Text}"  />
	</SelectDialog>
</core:FragmentDefinition>