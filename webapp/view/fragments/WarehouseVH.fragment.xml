<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<SelectDialog
		title="{i18n>Warehouses}"
		items="{
			path: 'valueHelpModel>/WareHouseNumberVHSet'
		}"
		search=".onValueHelpSearch($event,'WH')"
		confirm=".onValueHelpConfirm($event,'WH')"
		cancel=".onValueHelpClose">
		<StandardListItem
			title="{valueHelpModel>WarehouseNumber}" 
			description="{valueHelpModel>WarehouseNumber_Text}" />
	</SelectDialog>
</core:FragmentDefinition>