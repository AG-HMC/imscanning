<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<SelectDialog
		title="{i18n>Products}"
		items="{
			path: 'valueHelpModel>/ProductVHSet'
		}"
		search=".onValueHelpSearch($event,'Product')"
		confirm=".onValueHelpConfirm($event,'Product')"
		cancel=".onValueHelpClose">
		<StandardListItem
			title="{valueHelpModel>Product}"
			description="{valueHelpModel>ProductDescription}" />
	</SelectDialog>
</core:FragmentDefinition>