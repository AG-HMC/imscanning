<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<SelectDialog
		title="{i18n>HandlingUnits}"
		items="{
			path: 'valueHelpModel>/HandlingUnitVHSet'
		}"
		search=".onValueHelpSearch($event,'HU')"
		confirm=".onValueHelpConfirm($event,'HU')"
		cancel=".onValueHelpClose">
		<StandardListItem
			title="{valueHelpModel>HandlingUnit}"
			description="{valueHelpModel>HandlingUnitType_Text}" />
	</SelectDialog>
</core:FragmentDefinition>