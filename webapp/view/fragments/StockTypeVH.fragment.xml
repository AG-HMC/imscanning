<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<SelectDialog
		title="{i18n>StockTypes}"
		items="{
			path: 'valueHelpModel>/StockTypeVHSet'
		}"
		search=".onValueHelpSearch($event,'Stock Type')"
		confirm=".onValueHelpConfirm($event,'Stock Type')"
		cancel=".onValueHelpClose">
		<StandardListItem
			title="{valueHelpModel>StockType}"
			description="{valueHelpModel>StockTypeText}"/>
	</SelectDialog>
</core:FragmentDefinition>