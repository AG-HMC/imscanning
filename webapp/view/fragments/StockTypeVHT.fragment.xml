<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<SelectDialog
		title="{i18n>StockTypes}"
		items="{
			path: '/ZSCM_I_StockTypeVH'
		}"
		search=".onValueHelpSearch($event,'Stock Type Target')"
		confirm=".onValueHelpConfirm($event,'Stock Type Target')"
		cancel=".onValueHelpClose">
		<StandardListItem
			title="{StockType}"
			description="{StockTypeText}"/>
	</SelectDialog>
</core:FragmentDefinition>